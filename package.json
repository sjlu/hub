{
  "name": "hub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "web": "./node_modules/node-dev/bin/node-dev ./bin/web",
    "worker": "./node_modules/node-dev/bin/node-dev ./bin/worker",
    "listener": "./node_modules/node-dev/bin/node-dev ./bin/listener",
    "build": "./node_modules/gulp/bin/gulp.js",
    "postinstall": "./node_modules/bower/bin/bower install",
    "deploy": "npm install; npm run build; npm run \"deploy web\"; npm run \"deploy worker\";",
    "deploy web": "./deploy.sh web",
    "deploy worker": "./deploy.sh worker",
    "deploy listener": "./deploy.sh listener",
    "restart web": "./node_modules/naught/lib/main.js deploy ./naught-web.ipc",
    "restart worker": "./node_modules/naught/lib/main.js deploy ./naught-worker.ipc",
    "start web": "./node_modules/naught/lib/main.js start --pid-file naught-web.pid --ipc-file naught-web.ipc --log ./log/naught-web.log --stdout ./log/stdout-web.log --stderr ./log/stderr-web.log bin/web",
    "start worker": "./node_modules/naught/lib/main.js start --pid-file naught-worker.pid --ipc-file naught-worker.ipc --log ./log/naught-worker.log --stdout ./log/stdout-worker.log --stderr ./log/stderr-worker.log bin/worker",
    "stop web": "./node_modules/naught/lib/main.js stop ./naught-web.ipc",
    "stop worker": "./node_modules/naught/lib/main.js stop ./naught-worker.ipc",
    "gulp": "./node_modules/gulp/bin/gulp.js; ./node_modules/gulp/bin/gulp.js watch"
  },
  "dependencies": {
    "MD5": "^1.2.1",
    "URIjs": "^1.14.1",
    "async": "^0.9.0",
    "aws-sdk": "^2.1.4",
    "bcrypt": "^0.8.0",
    "body-parser": "~1.0.0",
    "bower": "^1.3.12",
    "connect-redis": "^2.1.0",
    "cookie-parser": "~1.0.1",
    "debug": "^0.7.4",
    "dotenv": "^0.4.0",
    "express": "~4.2.0",
    "express-flash": "0.0.2",
    "express-session": "^1.9.3",
    "gulp": "^3.8.10",
    "gulp-angular-templatecache": "^1.5.0",
    "gulp-jade": "^0.10.0",
    "gulp-less": "^1.3.7",
    "gulp-livereload": "^3.0.2",
    "gulp-minify-css": "^0.3.11",
    "gulp-plumber": "^0.6.6",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-stylus": "^1.3.4",
    "gulp-uglifyjs": "^0.5.0",
    "gulp-watch": "^3.0.0",
    "jade": "~1.3.0",
    "lodash": "^2.4.1",
    "moment": "^2.8.4",
    "mongoose": "^3.8.20",
    "mongoose-text-search": "0.0.2",
    "mongoose-validator": "^1.0.3",
    "morgan": "~1.0.0",
    "naught": "^1.5.0",
    "nib": "^1.0.4",
    "qued": "0.0.10",
    "redis": "^0.12.1",
    "request": "^2.51.0",
    "spark": "^0.2.4",
    "spark-protocol": "^0.1.5",
    "static-favicon": "~1.0.0",
    "uid": "0.0.2",
    "validator": "^3.24.0",
    "winston": "^0.8.3"
  },
  "devDependencies": {
    "node-dev": "^2.6.0"
  }
}
